#include "new.h"
#define N 100

int main()
{
    int i;
    int j;
    int k;
    int l;
    int **Tableau;

unsigned pc_0;
unsigned upper_bound_0 = Ehrhart0(N);
unsigned first_iteration_0 = 1;
#pragma omp parallel for private(i,j,k) firstprivate(first_iteration_0) schedule(static)
for (pc_0 = 1; pc_0 <= upper_bound_0; pc_0++)
{
	if (first_iteration_0)
	{
		i = trahrhe_i0(pc_0,N);
		first_iteration_0 = 0;
	}
	
	for (j=i;j<=42;j++) {
	  for (k=-N+1;;k++) {
	    (void)(i);
	    Tableau[i][j] = 0;
	  }
	}

	j++;
	if (j < min(42,N-1))
	{
		i++;
		j = i + 1;
	}
}

    return 0;
}
